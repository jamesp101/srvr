{
   "type": "sqlite",
   "database": "./data/database.db",
   "synchronize": true,
   "logging": false,
   "useUnifiedTopology": true,
   "entities": [
      "src/entity/**/*.ts"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli":{
      "migrationsDir": "src/migration/"
   }
}